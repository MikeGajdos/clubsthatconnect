{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "boiler-plate",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html -p 3000 --open",
    "clean": "rm -rf dist/*",
    "build:parcel": "parcel build src/index.html  --public-url ./",
    "build": "npm run clean && npm run build:parcel"
  },
  "keywords": [
    "parcel",
    "d3"
  ],
  "author": "Mike",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/runtime-corejs2": "^7.14.6",
    "autoprefixer": "^9.8.6",
    "parcel-bundler": "^1.12.4",
    "postcss": "^8.3.6",
    "rimraf": "^2.7.1"
  },
  "dependencies": {
    "d3": "^6.7.0",
    "gsap": "^3.7.1",
    "locomotive-scroll": "^4.1.1",
    "lodash-es": "^4.17.21",
    "splitting": "^1.0.6",
    "topojson-client": "^3.1.0"
  }
}
